<p id="notice"><%= notice %></p>

<h1>Testblogs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Message</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testblogs.each do |testblog| %>
      <tr>
        <td><%= testblog.Title %></td>
        <td><%= testblog.Message %></td>
        <td><%= testblog.Email %></td>
        <td><%= link_to 'Show', testblog %></td>
        <td><%= link_to 'Edit', edit_testblog_path(testblog) %></td>
        <td><%= link_to 'Destroy', testblog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testblog', new_testblog_path %>
